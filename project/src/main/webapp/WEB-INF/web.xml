<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" version="3.0">
  <display-name>project</display-name>
  <listener>
    <listener-class>org.jboss.weld.environment.servlet.Listener</listener-class>
  </listener>
  <filter>
    <filter-name>sitemesh</filter-name>
    <filter-class>com.opensymphony.module.sitemesh.filter.PageFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>sitemesh</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>
  <session-config>
    <tracking-mode>COOKIE</tracking-mode>
  </session-config>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <description></description>
    <display-name>HomeController</display-name>
    <servlet-name>HomeController</servlet-name>
    <servlet-class>controller.web.HomeController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>HomeController</servlet-name>
    <url-pattern>/HomeController</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>AdminHomeController</display-name>
    <servlet-name>AdminHomeController</servlet-name>
    <servlet-class>controller.admin.AdminHomeController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AdminHomeController</servlet-name>
    <url-pattern>/AdminHomeController</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>CategoryAPI</display-name>
    <servlet-name>CategoryAPI</servlet-name>
    <servlet-class>admin.api.CategoryAPI</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>CategoryAPI</servlet-name>
    <url-pattern>/CategoryAPI</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>AdminCategoryController</display-name>
    <servlet-name>AdminCategoryController</servlet-name>
    <servlet-class>controller.admin.AdminCategoryController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AdminCategoryController</servlet-name>
    <url-pattern>/AdminCategoryController</url-pattern>
  </servlet-mapping>
</web-app>